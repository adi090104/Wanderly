{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "VITE_PORT": "3000",
    "VITE_API_URL": "https://wanderly-4mvr.vercel.app"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
