{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://wanderly-new-production.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "VITE_PORT": "3000",
    "VITE_API_URL": "https://wanderly-new-production.up.railway.app"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
